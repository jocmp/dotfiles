  adp = add -p
  co = checkout
  st = status
  ci = commit -S
  pu = pull --rebase
  ps = push
  br = branch
  gu = reset --soft HEAD~1
  can = commit -S --amend --no-edit
[branch]
 sort = committerdate
[color]
  diff = auto
  status = auto
  branch = auto
[core]
	editor = em
  autocrlf = input
	pager = diff-so-fancy | less --tabs=4 -RFX
[apply]
  whitespace = nowarn
[web]
  browser = open
